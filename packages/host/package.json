{
  "name": "host",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "webpack-cli serve",
    "build": "webpack --mode production",
    "serve": "serve dist -p 3001",
    "clean": "rm -rf dist",
    "tsc": "tsc",
    "tsc:check": "tsc --noEmit true --declaration false --emitDeclarationOnly false",
    "build:js": "BABEL_ENV=production babel src --extensions \".js,.jsx,.ts,.tsx\" --ignore \"src/**/*.d.ts\" -d lib -s",
    "compile": "rm -rf lib && yarn build:js && yarn tsc"
  },
  "dependencies": {
    "library": "*",
    "mobx": "5",
    "mobx-react": "6",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "shared-context": "*"
  },
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@types/react": "^18.0.21",
    "babel-loader": "8.2.5",
    "eslint": "^8.23.1",
    "eslint-config-react-app": "^7.0.1",
    "html-webpack-plugin": "5.5.0",
    "serve": "13.0.4",
    "typescript": "^4.8.3",
    "webpack": "5.74.0",
    "webpack-cli": "4.10.0",
    "webpack-dev-server": "^4.11.0"
  },
  "browserslist": [
    "last 2 version",
    "> 1%",
    "not dead",
    "not ie <= 10",
    "iOS >13",
    "chrome >= 71"
  ]
}
