{
  "name": "@mf/main",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "webpack serve --mode production",
    "build": "webpack --mode development",
    "serve": "serve dist -p 3001",
    "clean": "rm -rf dist",
    "tsc": "tsc",
    "tsc:check": "tsc --noEmit true --declaration false --emitDeclarationOnly false",
    "build:js": "BABEL_ENV=production babel src --extensions \".js,.jsx,.ts,.tsx\" --ignore \"src/**/*.d.ts\" -d lib -s",
    "compile": "rm -rf lib && yarn build:js && yarn tsc"
  },
  "dependencies": {
    "@mf/core": "*",
    "@mf/flamingo": "*",
    "@mf/shared": "*",
    "mobx": "5",
    "mobx-react": "6",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "^6.4.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.10",
    "@types/react": "^18.0.26",
    "babel-loader": "9.1.0",
    "eslint": "^8.29.0",
    "eslint-config-react-app": "^7.0.1",
    "html-webpack-plugin": "5.5.0",
    "react-refresh": "^0.14.0",
    "serve": "14.1.2",
    "typescript": "^4.9.3",
    "webpack": "5.75.0",
    "webpack-cli": "5.0.1",
    "webpack-dev-server": "^4.11.1"
  },
  "browserslist": [
    "last 2 version",
    "> 1%",
    "not dead",
    "not ie <= 10",
    "iOS >13",
    "chrome >= 71"
  ]
}
