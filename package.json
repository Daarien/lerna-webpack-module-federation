{
  "name": "root",
  "private": true,
  "packageManager": "yarn@3.2.3",
  "workspaces": [
    "packages/*"
  ],
  "devDependencies": {
    "@types/react": "^18.0.21",
    "lerna": "^5.5.0"
  },
  "scripts": {
    "start:host": "lerna run start --scope=host",
